#!/usr/bin/make -f

# See debhelper(7) (uncomment to enable).
# Output every command that modifies files on the build system.
#export DH_VERBOSE = 1

%:
	dh $@

override_dh_install:
	dh_install sources/etc/haspd/* /etc/haspd
	dh_install sources/usr/* /usr/

override_dh_installsystemd:
	dh_installsystemd
	dh_installsystemd --name=aksusbd
	dh_installsystemd --name=hasplm

override_dh_installudev:
	dh_installudev
