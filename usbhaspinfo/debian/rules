#!/usr/bin/make -f

#export DH_VERBOSE=1
include /usr/share/dpkg/pkg-info.mk

%:
	dh $@

override_dh_install:
	cd $(CURDIR)/src && make
	dh_install src/usbhaspinfo /usr/sbin/
	dh_install src/nethasp.ini /etc/hasplm/

override_dh_auto_clean:
	cd $(CURDIR)/src && make clean
override_dh_strip:
	dh_strip --no-automatic-dbgsym